{
  "name": "sagefy-client",
  "version": "1.0.0",
  "private": false,
  "description": "Learn anything, customized for you. An always free.",
  "license": "Apache-2.0",
  "author": "Kevin Heis and contributors",
  "main": "index.js",
  "repository": "https://github.com/heiskr/sagefy.git",
  "scripts": {
    "build-client": "yarn run compile-content && webpack ./index.client.js -o ../nginx/statics/index2.js",
    "build-server": "babel . --out-dir server_dist --ignore node_modules,server_dist",
    "build-styles": "stylus --include-css --use hsluv-stylus --include . ./index.styl -o ../nginx/statics/index2.css",
    "precommit": "pretty-quick --staged",
    "compile-content": "yaml2json \"../content/\" --recursive --save && mkdir -p ./content && mv ../content/*.json ./content/",
    "compress-scripts": "yarn run build-client && uglifyjs --compress --mangle --output ../nginx/statics/index2.js -- ../nginx/statics/index2.js",
    "compress-styles": "yarn run build-styles && cleancss ../nginx/statics/index2.css -o ../nginx/statics/index2.css",
    "deploy": "concurrently -p \"[{name}]\" --names compress-styles,compress-scripts,build-server \"yarn run compress-styles\" \"yarn run compress-scripts\" \"yarn run build-server\"",
    "lint": "eslint \"**/*.js\"",
    "start": "yarn run compile-content && concurrently -p \"[{name}]\" --names watch-styles,watch-client \"yarn run watch-styles\" \"yarn run watch-client\"",
    "test": "yarn run compile-content && concurrently -p \"[{name}]\" --names lint,jest \"yarn run lint\" \"yarn run jest\"",
    "watch-client": "webpack --watch ./index.client.js -o ../nginx/statics/index2.js",
    "watch-styles": "stylus --include-css --use hsluv-stylus --include . -w ./index.styl -o ../nginx/statics/index2.css"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "cookie-parser": "1.4.3",
    "express": "4.16.3",
    "font-awesome": "4.7.0",
    "jsdom": "11.11.0",
    "lodash.capitalize": "4.2.1",
    "lodash.clone": "4.5.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.compact": "3.0.1",
    "lodash.flattendeep": "4.4.0",
    "lodash.frompairs": "4.0.1",
    "lodash.get": "4.4.2",
    "lodash.isarray": "4.0.0",
    "lodash.isnumber": "3.0.3",
    "lodash.isplainobject": "4.0.6",
    "lodash.mapvalues": "4.6.0",
    "lodash.merge": "4.6.1",
    "lodash.omit": "4.5.0",
    "lodash.snakecase": "4.1.1",
    "lodash.topairs": "4.3.0",
    "lodash.transform": "4.6.0",
    "normalize.css": "8.0.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-listen": "6.0.1",
    "redux-schemad": "0.2.1",
    "reselect": "3.0.1",
    "timeago.js": "3.0.2"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "clean-css": "4.1.11",
    "concurrently": "3.5.1",
    "eslint": "5.1.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "glob": "7.1.2",
    "hsluv-stylus": "0.0.1",
    "husky": "0.14.3",
    "jest": "23.4.1",
    "json-loader": "0.5.7",
    "prettier": "1.13.7",
    "pretty-quick": "1.6.0",
    "raw-loader": "0.5.1",
    "stylus": "0.54.5",
    "uglify-js": "3.4.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "yamljs": "0.3.0"
  }
}
