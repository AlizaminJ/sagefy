.my-sets__list
    @extend $list-options

.my-sets__engage-set
    float left
    @extend $button--good

.my-sets__my-set-right
    margin-left cols(2)

.my-sets__find-first-set
    @extend $button--good

.my-sets__info
    margin ($unit * 2) 0
    padding $unit ($unit * 2)
    background $base-color(95)
    border-radius $border-radius

.my-sets__info small
    color $base-color(45)
