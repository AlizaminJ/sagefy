{
  "name": "sagefy-client",
  "version": "2.0.0",
  "description": "Sagefy. Learn anything, adapted for you. Free.",
  "main": "index.js",
  "scripts": {
    "build-css": "cat node_modules/drab.css/css/drabutil.css partial.css > ../nginx/sagefy.css",
    "min-css": "cleancss -o ../nginx/sagefy.min.css ../nginx/sagefy.css",
    "prepublish": "npm run build-css && npm run min-css",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagefy/sagefy.git"
  },
  "author": "Kevin Heis and contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sagefy/sagefy/issues"
  },
  "homepage": "https://github.com/sagefy/sagefy#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "drab.css": "^1.9.0",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-react-views": "^0.11.0",
    "graphql-request": "^1.8.2",
    "jsonwebtoken": "^8.5.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.topairs": "^4.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-feather": "^1.1.6",
    "react-markdown": "^4.0.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "shuffle-seed": "^1.1.6",
    "timeago-react": "^2.0.1",
    "uuid": "^3.3.2",
    "uuid58": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "clean-css": "^4.2.1",
    "clean-css-cli": "^4.2.1",
    "jest": "^24.8.0",
    "superagent": "^4.1.0",
    "supertest": "^3.4.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ],
      "@babel/preset-react"
    ]
  }
}
